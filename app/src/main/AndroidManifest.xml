<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bec.hamilton" >
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FoodActivity"
            android:label="@string/title_activity_food" >
        </activity>
        <activity
            android:name=".EmergencyActivity"
            android:label="@string/title_activity_emergency" >
        </activity>
        <activity
            android:name=".UberActivity"
            android:label="@string/title_activity_uber" >
        </activity>
        <activity
            android:name=".MainProviderActivity"
            android:label="@string/title_activity_main_provider" >
        </activity>
        <activity
            android:name=".LandingActivity"
            android:label="@string/title_activity_landing" >
        </activity>
        <activity
            android:name=".MainVendorActivity"
            android:label="@string/title_activity_main_vendor" >
        </activity>
    </application>

</manifest>
